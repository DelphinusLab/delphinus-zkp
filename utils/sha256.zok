import "utils/pack/u32/nonStrictUnpack256" as unpack256u
import "hashes/sha256/sha256" as sha256

def main<N>(field[N][2] content) -> u32[8]:
    u32[N + 1][16] data = [
        ...[[0u32; 16]; N],
        [
            0x80000000,
            ...[0u32; 14],
            N * 512
        ]
    ]

    for u32 i in 0..N do
        data[i] = [
            ...unpack256u(content[i][0]),
            ...unpack256u(content[i][1])
        ]
    endfor

    return sha256(data)